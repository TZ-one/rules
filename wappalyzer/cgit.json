{
    "implies": "git",
    "name": "cgit",
    "matches": [
        {
            "search": "meta[generator]",
            "offset": 1,
            "regexp": "^cgit v([\\d.a-z-]+)$"
        },
        {
            "regexp": "<[^>]+id='cgit'"
        },
        {
            "offset": 1,
            "regexp": "generated by <a href='http://git\\.zx2c4\\.com/cgit/about/'>cgit v([\\d.a-z-]+)</a>"
        }
    ],
    "website": "http://git.zx2c4.com/cgit"
}