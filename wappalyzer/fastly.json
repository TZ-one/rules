{
    "website": "https://www.fastly.com",
    "name": "Fastly",
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-via-fastly:]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-fastly-request-id]"
        },
        {
            "regexp": "Fastly-SSL",
            "search": "headers[vary]"
        },
        {
            "regexp": "^cache-",
            "search": "headers[x-served-by]"
        },
        {
            "regexp": ".*",
            "search": "headers[fastly-debug-digest]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-fastly-origin]"
        }
    ]
}