{
    "name": "wpCache",
    "website": "https://wpcache.co",
    "implies": "PHP",
    "matches": [
        {
            "regexp": "<!--[^>]+wpCache"
        },
        {
            "search": "meta[generator]",
            "regexp": "wpCache"
        },
        {
            "search": "meta[keywords]",
            "regexp": "wpCache"
        },
        {
            "search": "headers[x-powered-by]",
            "regexp": "wpCache(?:/([\\d.]+))?",
            "offset": 1
        }
    ]
}