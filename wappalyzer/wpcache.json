{
    "implies": "PHP",
    "name": "wpCache",
    "matches": [
        {
            "search": "meta[keywords]",
            "regexp": "wpCache"
        },
        {
            "search": "meta[generator]",
            "regexp": "wpCache"
        },
        {
            "regexp": "<!--[^>]+wpCache"
        },
        {
            "search": "headers[x-powered-by]",
            "offset": 1,
            "regexp": "wpCache(?:/([\\d.]+))?"
        }
    ],
    "website": "https://wpcache.co"
}