{
    "name": "GOV.UK Frontend",
    "website": "https://design-system.service.gov.uk/",
    "matches": [
        {
            "offset": 1,
            "regexp": "<link[^>]* href=[^>]*?govuk-frontend(?:[^>]*?([0-9a-fA-F]{7,40}|[\\d]+(?:.[\\d]+(?:.[\\d]+)?)?)|)[^>]*?(?:\\.min)?\\.css"
        },
        {
            "certainty": 80,
            "regexp": "<body[^>]+govuk-template__body"
        },
        {
            "certainty": 10,
            "regexp": "<a[^>]+govuk-link"
        }
    ]
}