{
    "name": "Orchard Core",
    "website": "https://orchardcore.net",
    "matches": [
        {
            "regexp": "Orchard",
            "search": "meta[generator]"
        },
        {
            "regexp": "^Orchard$",
            "search": "headers[x-generator]"
        },
        {
            "regexp": "OrchardCore",
            "search": "headers[x-powered-by]"
        }
    ],
    "implies": "Microsoft ASP.NET"
}