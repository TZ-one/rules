{
    "name": "gitweb",
    "matches": [
        {
            "regexp": "static/gitweb\\.js$",
            "search": "script"
        },
        {
            "regexp": "gitweb(?:/([\\d.]+\\d))?",
            "search": "meta[generator]",
            "offset": 1
        },
        {
            "regexp": "<!-- git web interface version ([\\d.]+)?",
            "offset": 1
        }
    ],
    "implies": [
        "Perl",
        "git"
    ],
    "website": "http://git-scm.com"
}