{
    "website": "http://google.com/fonts",
    "name": "Google Font API",
    "matches": [
        {
            "regexp": "<link[^>]* href=[^>]+fonts\\.(?:googleapis|google)\\.com"
        },
        {
            "search": "script",
            "regexp": "googleapis\\.com/.+webfont"
        }
    ]
}