{
    "name": "Phusion Passenger",
    "website": "https://phusionpassenger.com",
    "matches": [
        {
            "regexp": "Phusion Passenger(?:\\(R\\))? ?([\\d.]+)?",
            "offset": 1,
            "search": "headers[x-powered-by]"
        },
        {
            "regexp": "Phusion Passenger ([\\d.]+)",
            "offset": 1,
            "search": "headers[server]"
        }
    ]
}