{
    "name": "MODX",
    "implies": "PHP",
    "matches": [
        {
            "regexp": "<a[^>]+>Powered by MODX</a>"
        },
        {
            "regexp": "<(?:link|script)[^>]+assets/snippets/",
            "certainty": 20
        },
        {
            "regexp": "<form[^>]+id=\"ajaxSearch_form",
            "certainty": 20
        },
        {
            "regexp": "<input[^>]+id=\"ajaxSearch_input",
            "certainty": 20
        },
        {
            "offset": 1,
            "search": "meta[generator]",
            "regexp": "MODX[^\\d.]*([\\d.]+)?"
        },
        {
            "search": "headers[x-powered-by]",
            "regexp": "^MODX"
        }
    ],
    "website": "http://modx.com"
}