{
    "name": "Shopify",
    "website": "http://shopify.com",
    "matches": [
        {
            "regexp": ".*",
            "search": "meta[shopify-digital-wallet]"
        },
        {
            "regexp": ".*",
            "search": "meta[shopify-checkout-api-token]"
        },
        {
            "regexp": ".*",
            "search": "cookies[_shopify_s]"
        },
        {
            "regexp": ".*",
            "search": "cookies[_shopify_y]"
        },
        {
            "regexp": "shopifyTag",
            "search": "script"
        },
        {
            "regexp": ".*",
            "certainty": 50,
            "search": "headers[x-shopid]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-shopify-stage]"
        }
    ]
}