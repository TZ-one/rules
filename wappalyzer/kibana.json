{
    "name": "Kibana",
    "implies": [
        "Node.js",
        "Elasticsearch"
    ],
    "website": "http://www.elastic.co/products/kibana",
    "matches": [
        {
            "regexp": "<title>Kibana</title>"
        },
        {
            "regexp": "kibana",
            "search": "headers[kbn-name]"
        },
        {
            "regexp": "^([\\d.]+)$",
            "offset": 1,
            "search": "headers[kbn-version]"
        }
    ]
}