{
    "website": "https://vtex.com/",
    "name": "VTEX",
    "matches": [
        {
            "search": "headers[powered]",
            "regexp": "vtex"
        },
        {
            "search": "headers[server]",
            "regexp": "^VTEX IO$"
        },
        {
            "search": "script",
            "regexp": "io\\.vtex\\.com\\.br"
        },
        {
            "search": "cookies[vtex_session]",
            "regexp": ".*"
        },
        {
            "search": "cookies[VtexFingerPrint]",
            "regexp": ".*"
        },
        {
            "search": "cookies[VtexStoreVersion]",
            "regexp": ".*"
        },
        {
            "search": "cookies[VtexWorkspace]",
            "regexp": ".*"
        }
    ]
}