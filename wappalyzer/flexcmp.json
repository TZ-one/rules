{
    "name": "FlexCMP",
    "website": "http://www.flexcmp.com/cms/home",
    "matches": [
        {
            "regexp": "^FlexCMP",
            "search": "meta[generator]"
        },
        {
            "offset": 1,
            "regexp": "<!--[^>]+FlexCMP[^>v]+v\\. ([\\d.]+)"
        },
        {
            "regexp": "FlexCMP.+\\[v\\. ([\\d.]+)",
            "search": "headers[x-powered-by]",
            "offset": 1
        },
        {
            "regexp": ".*",
            "search": "headers[x-flex-lang]"
        }
    ]
}