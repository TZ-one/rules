{
    "name": "Saber",
    "implies": "Vue.js",
    "website": "https://saber.land/",
    "matches": [
        {
            "regexp": "<div [^>]*id=\"_saber\""
        },
        {
            "regexp": "^Saber v([\\d.]+)$",
            "offset": 1,
            "search": "meta[generator]"
        }
    ]
}