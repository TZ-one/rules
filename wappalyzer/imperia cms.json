{
    "name": "imperia CMS",
    "matches": [
        {
            "search": "meta[x-imperia-live-info]",
            "regexp": ".*"
        },
        {
            "offset": 1,
            "search": "meta[generator]",
            "regexp": "^IMPERIA\\s([\\d\\.\\_]+)"
        }
    ],
    "implies": "Perl",
    "website": "https://www.pirobase-imperia.com/de/solutions/imperia-cms"
}