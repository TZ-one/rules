{
    "name": "Datadome",
    "website": "https://datadome.co/",
    "matches": [
        {
            "search": "headers[server]",
            "regexp": "^DataDome$"
        },
        {
            "search": "headers[x-datadome]",
            "regexp": ".*"
        },
        {
            "search": "headers[x-datadome-cid]",
            "regexp": ".*"
        },
        {
            "search": "cookies[datadome]",
            "regexp": ".*"
        },
        {
            "search": "cookies[datadome-_zldt]",
            "regexp": ".*"
        },
        {
            "search": "cookies[datadome-_zldp]",
            "regexp": ".*"
        },
        {
            "search": "script",
            "regexp": "^https://ct\\.datadome\\.co/[a-z]\\.js$"
        }
    ]
}