{
    "name": "Phabricator",
    "implies": "PHP",
    "website": "http://phacility.com",
    "matches": [
        {
            "search": "script",
            "regexp": "/phabricator/[a-f0-9]{8}/rsrc/js/phui/[a-z-]+\\.js$"
        },
        {
            "regexp": "<[^>]+(?:class|id)=\"phabricator-"
        },
        {
            "search": "cookies[phsid]",
            "regexp": ".*"
        }
    ]
}