{
    "name": "SPIP",
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-spip-cache]"
        },
        {
            "offset": 1,
            "regexp": "SPIP ([\\d.]+) @",
            "search": "headers[composed-by]"
        },
        {
            "offset": 1,
            "regexp": "(?:^|\\s)SPIP(?:\\s([\\d.]+(?:\\s\\[\\d+\\])?))?",
            "search": "meta[generator]"
        }
    ],
    "implies": "PHP",
    "website": "http://www.spip.net"
}