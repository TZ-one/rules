{
    "implies": "PHP",
    "name": "MediaWiki",
    "website": "https://www.mediawiki.org",
    "matches": [
        {
            "regexp": "<body[^>]+class=\"mediawiki\""
        },
        {
            "regexp": "<(?:a|img)[^>]+>Powered by MediaWiki</a>"
        },
        {
            "regexp": "<a[^>]+/Special:WhatLinksHere/"
        },
        {
            "offset": 1,
            "regexp": "^MediaWiki ?(.+)$",
            "search": "meta[generator]"
        }
    ]
}