{
    "website": "https://developers.google.com/identity/sign-in/web",
    "matches": [
        {
            "regexp": "accounts\\.google\\.com/gsi/client",
            "search": "script"
        },
        {
            "regexp": ".*",
            "search": "meta[google-signin-client_id]"
        },
        {
            "regexp": ".*",
            "search": "meta[google-signin-scope]"
        }
    ],
    "name": "Google Sign-in"
}