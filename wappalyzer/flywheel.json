{
    "name": "Flywheel",
    "implies": "WordPress",
    "website": "https://getflywheel.com",
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-fw-static]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-fw-hash]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-fw-type]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-fw-serve]"
        },
        {
            "regexp": "^Flywheel(?:/([\\d.]+))?",
            "search": "headers[x-fw-server]",
            "offset": 1
        }
    ]
}