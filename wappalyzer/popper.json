{
    "website": "https://popper.js.org",
    "name": "Popper",
    "matches": [
        {
            "offset": 1,
            "regexp": "<script [^>]*src=\"[^\"]*/popper\\.js/([0-9.]+)"
        },
        {
            "offset": 1,
            "search": "script",
            "regexp": "/popper\\.js/([0-9.]+)"
        }
    ]
}