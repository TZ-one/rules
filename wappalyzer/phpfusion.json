{
    "name": "PHPFusion",
    "website": "https://phpfusion.com",
    "matches": [
        {
            "regexp": "Powered by <a href=\"[^>]+phpfusion"
        },
        {
            "regexp": "Powered by <a href=\"[^>]+php-fusion"
        },
        {
            "offset": 1,
            "search": "headers[x-phpfusion]",
            "regexp": "(.+)$"
        },
        {
            "offset": 1,
            "search": "headers[x-powered-by]",
            "regexp": "PHPFusion (.+)$"
        }
    ],
    "implies": [
        "PHP",
        "MySQL"
    ]
}