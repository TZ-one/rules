        {
            "offset": 1,
            "regexp": "vue[.-]([\\d.]*\\d)[^/]*\\.js",
            "search": "script"
        },
        {
            "offset": 1,
            "regexp": "(?:/([\\d.]+))?/vue(?:\\.min)?\\.js",
