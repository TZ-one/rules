{
    "name": "Ckan",
    "website": "https://ckan.org/",
    "matches": [
        {
            "offset": 1,
            "search": "meta[generator]",
            "regexp": "^ckan ?([0-9.]+)$"
        },
        {
            "search": "headers[access-control-allow-headers]",
            "regexp": "X-CKAN-API-KEY"
        },
        {
            "search": "headers[link]",
            "regexp": "<http://ckan\\.org/>; rel=shortlink"
        }
    ],
    "implies": [
        "Python",
        "Solr",
        "Java",
        "PostgreSQL"
    ]
}