{
    "name": "WEBDEV",
    "website": "https://www.windev.com/webdev/index.html",
    "matches": [
        {
            "regexp": "<!-- [a-zA-Z0-9_]+ [\\d/]+ [\\d:]+ WebDev \\d\\d ([\\d.]+) -->",
            "offset": 1
        },
        {
            "search": "meta[generator]",
            "regexp": "^WEBDEV$"
        },
        {
            "search": "headers[webdevsrc]",
            "regexp": ".*"
        }
    ]
}