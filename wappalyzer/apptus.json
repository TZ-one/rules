{
    "name": "Apptus",
    "website": "https://www.apptus.com",
    "matches": [
        {
            "search": "cookies[apptus.customerKey]",
            "regexp": ".*"
        },
        {
            "search": "cookies[apptus.sessionKey]",
            "regexp": ".*"
        },
        {
            "search": "script",
            "regexp": "cdn\\.esales\\.apptus\\.com.+(?:apptus-esales-api-([\\d.]+))\\.min\\.js",
            "offset": 1
        }
    ]
}