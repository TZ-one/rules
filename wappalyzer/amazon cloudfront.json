{
    "name": "Amazon Cloudfront",
    "implies": "Amazon Web Services",
    "website": "http://aws.amazon.com/cloudfront/",
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-amz-cf-id]"
        },
        {
            "regexp": "\\(CloudFront\\)$",
            "search": "headers[via]"
        }
    ]
}