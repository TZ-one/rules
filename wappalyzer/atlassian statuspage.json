{
    "name": "Atlassian Statuspage",
    "website": "https://www.statuspage.io/",
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-statuspage-version]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-statuspage-skip-logging]"
        },
        {
            "regexp": "<a[^>]*href=\"https?://(?:www\\.)?statuspage\\.io/powered-by[^>]+>"
        }
    ]
}