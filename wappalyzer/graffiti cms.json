{
    "implies": "Microsoft ASP.NET",
    "matches": [
        {
            "regexp": ".*",
            "search": "cookies[graffitibot]"
        },
        {
            "regexp": "/graffiti\\.js",
            "search": "script"
        },
        {
            "offset": 1,
            "regexp": "Graffiti CMS ([^\"]+)",
            "search": "meta[generator]"
        }
    ],
    "name": "Graffiti CMS",
    "website": "http://graffiticms.codeplex.com"
}