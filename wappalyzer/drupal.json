{
    "name": "Drupal",
    "implies": "PHP",
    "matches": [
        {
            "search": "meta[generator]",
            "regexp": "^Drupal(?:\\s([\\d.]+))?",
            "offset": 1
        },
        {
            "search": "headers[x-generator]",
            "regexp": "^Drupal(?:\\s([\\d.]+))?",
            "offset": 1
        },
        {
            "search": "headers[expires]",
            "regexp": "19 Nov 1978"
        },
        {
            "search": "headers[x-drupal-cache]",
            "regexp": ".*"
        },
        {
            "regexp": "<(?:link|style)[^>]+\"/sites/(?:default|all)/(?:themes|modules)/"
        }
    ],
    "website": "https://drupal.org"
}