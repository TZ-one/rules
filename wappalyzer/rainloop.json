{
    "name": "RainLoop",
    "website": "https://www.rainloop.net/",
    "implies": "PHP",
    "matches": [
        {
            "regexp": "^RainLoop",
            "search": "headers[server]"
        },
        {
            "offset": 1,
            "regexp": "^([0-9.]+)$",
            "search": "meta[rlAppVersion]"
        },
        {
            "offset": 1,
            "regexp": "<link[^>]href=\"rainloop/v/([0-9.]+)/static/apple-touch-icon\\.png/>"
        },
        {
            "offset": 1,
            "regexp": "^rainloop/v/([0-9.]+)/",
            "search": "script"
        }
    ]
}