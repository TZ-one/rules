{
    "name": "Oxygen",
    "website": "https://oxygenbuilder.com",
    "matches": [
        {
            "regexp": "<body class=(?:\"|')[^\"']*oxygen-body"
        },
        {
            "regexp": "<link [^>]*href=(?:\"|')[^>]*wp-content/plugins/oxygen/"
        },
        {
            "search": "script",
            "regexp": "wp-content/plugins/oxygen"
        }
    ]
}