{
    "name": "Orckestra",
    "implies": "Microsoft ASP.NET",
    "matches": [
        {
            "search": "headers[x-orckestra-commerce]",
            "regexp": ".NET Client"
        },
        {
            "search": "headers[x-powered-by]",
            "regexp": "Orckestra"
        },
        {
            "search": "meta[generator]",
            "regexp": "^C1 CMS Foundation - Free Open Source from Orckestra and https://github.com/Orckestra/C1-CMS-Foundation$"
        }
    ],
    "website": "https://www.orckestra.com"
}