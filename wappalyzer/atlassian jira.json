{
    "name": "Atlassian Jira",
    "implies": "Java",
    "matches": [
        {
            "offset": 1,
            "search": "meta[data-version]",
            "regexp": "([\\d.]+)",
            "certainty": 0
        },
        {
            "search": "meta[application-name]",
            "regexp": "JIRA"
        }
    ],
    "website": "http://www.atlassian.com/software/jira/overview/"
}