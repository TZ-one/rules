{
    "name": "Vue Storefront",
    "website": "https://www.vuestorefront.io/",
    "matches": [
        {
            "regexp": "^Vue Storefront ([0-9.]+)?$",
            "search": "meta[generator]",
            "offset": 1
        },
        {
            "regexp": "vsf-layout",
            "search": "script"
        }
    ],
    "implies": "Vue.js"
}