{
    "name": "Stripe",
    "website": "http://stripe.com",
    "matches": [
        {
            "regexp": "<input[^>]+data-stripe"
        },
        {
            "regexp": ".*",
            "search": "cookies[__stripe_mid]"
        },
        {
            "regexp": ".*",
            "search": "cookies[__stripe_sid]"
        },
        {
            "regexp": "js\\.stripe\\.com",
            "search": "script"
        }
    ]
}