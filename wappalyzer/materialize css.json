{
    "website": "http://materializecss.com",
    "name": "Materialize CSS",
    "matches": [
        {
            "regexp": "<link[^>]* href=\"[^\"]*materialize(?:\\.min)?\\.css"
        },
        {
            "regexp": "materialize(?:\\.min)?\\.js",
            "search": "script"
        }
    ]
}