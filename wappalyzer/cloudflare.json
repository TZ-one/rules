{
    "website": "http://www.cloudflare.com",
    "name": "Cloudflare",
    "matches": [
        {
            "regexp": "^cloudflare$",
            "search": "headers[server]"
        },
        {
            "regexp": ".*",
            "search": "headers[cf-cache-status]"
        },
        {
            "regexp": ".*",
            "search": "headers[cf-ray]"
        },
        {
            "regexp": ".*",
            "search": "cookies[__cfduid]"
        }
    ]
}