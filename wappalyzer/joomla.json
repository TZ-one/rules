{
    "implies": "PHP",
    "name": "Joomla",
    "website": "https://www.joomla.org",
    "matches": [
        {
            "certainty": 50,
            "regexp": "(?:<div[^>]+id=\"wrapper_r\"|<(?:link|script)[^>]+(?:feed|components)/com_|<table[^>]+class=\"pill)"
        },
        {
            "offset": 1,
            "search": "headers[x-content-encoded-by]",
            "regexp": "Joomla! ([\\d.]+)"
        },
        {
            "offset": 1,
            "search": "meta[generator]",
            "regexp": "Joomla!(?: ([\\d.]+))?"
        }
    ]
}