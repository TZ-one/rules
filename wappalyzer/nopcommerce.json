{
    "name": "nopCommerce",
    "implies": "Microsoft ASP.NET",
    "website": "http://www.nopcommerce.com",
    "matches": [
        {
            "regexp": "(?:<!--Powered by nopCommerce|Powered by: <a[^>]+nopcommerce)"
        },
        {
            "search": "meta[generator]",
            "regexp": "^nopCommerce$"
        },
        {
            "search": "cookies[Nop.customer]",
            "regexp": ".*"
        }
    ]
}