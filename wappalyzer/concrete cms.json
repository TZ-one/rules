{
    "name": "Concrete CMS",
    "implies": "PHP",
    "matches": [
        {
            "search": "cookies[CONCRETE5]",
            "regexp": ".*"
        },
        {
            "search": "script",
            "regexp": "/concrete/js/"
        },
        {
            "offset": 1,
            "search": "meta[generator]",
            "regexp": "^concrete5(?: - ([\\d.]+)$)?"
        }
    ],
    "website": "https://www.concretecms.com"
}