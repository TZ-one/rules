{
    "name": "Edgio App Platform",
    "website": "https://edg.io",
    "matches": [
        {
            "regexp": ".*",
            "search": "cookies[layer0_bucket]"
        },
        {
            "regexp": ".*",
            "search": "cookies[layer0_destination]"
        },
        {
            "regexp": ".*",
            "search": "cookies[layer0_eid]"
        },
        {
            "offset": 1,
            "regexp": "^\\d+ ([\\d.]+) ",
            "search": "headers[x-0-version]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-0-status]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-0-t]"
        }
    ]
}