{
    "implies": "PHP",
    "name": "Contao",
    "website": "http://contao.org",
    "matches": [
        {
            "regexp": "^Contao Open Source CMS$",
            "search": "meta[generator]"
        },
        {
            "regexp": "<!--[^>]+powered by (?:TYPOlight|Contao)[^>]*-->"
        },
        {
            "regexp": "<link[^>]+(?:typolight|contao)\\.css"
        }
    ]
}