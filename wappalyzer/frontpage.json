{
    "website": "http://office.microsoft.com/frontpage",
    "name": "FrontPage",
    "matches": [
        {
            "regexp": "^FrontPage\\.",
            "search": "meta[ProgId]"
        },
        {
            "regexp": "Microsoft FrontPage(?:\\s((?:Express )?[\\d.]+))?",
            "offset": 1,
            "search": "meta[generator]"
        }
    ]
}