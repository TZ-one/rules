{
    "name": "Skilldo",
    "implies": [
        "PHP",
        "MySQL"
    ],
    "website": "https://developers.sikido.vn/docs/cms/",
    "matches": [
        {
            "regexp": "^Skilldo$",
            "search": "headers[cms-name]"
        },
        {
            "regexp": "([\\d\\.]+)",
            "offset": 1,
            "search": "headers[cms-version]",
            "certainty": 0
        }
    ]
}