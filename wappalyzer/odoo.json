{
    "name": "Odoo",
    "website": "http://odoo.com",
    "implies": [
        "Python",
        "PostgreSQL",
        "Node.js",
        "Less"
    ],
    "matches": [
        {
            "regexp": "Odoo",
            "search": "meta[generator]"
        },
        {
            "certainty": 25,
            "regexp": "<link[^>]* href=[^>]+/web/css/(?:web\\.assets_common/|website\\.assets_frontend/)"
        },
        {
            "certainty": 25,
            "regexp": "/web/js/(?:web\\.assets_common/|website\\.assets_frontend/)",
            "search": "script"
        }
    ]
}