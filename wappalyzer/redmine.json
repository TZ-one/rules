{
    "name": "Redmine",
    "implies": "Ruby on Rails",
    "website": "http://www.redmine.org",
    "matches": [
        {
            "regexp": "Redmine",
            "search": "meta[description]"
        },
        {
            "regexp": "Powered by <a href=\"[^>]+Redmine"
        },
        {
            "regexp": ".*",
            "search": "cookies[_redmine_session]"
        }
    ]
}