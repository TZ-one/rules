{
    "name": "BIGACE",
    "matches": [
        {
            "regexp": "(?:Powered by <a href=\"[^>]+BIGACE|<!--\\s+Site is running BIGACE)"
        },
        {
            "search": "meta[generator]",
            "offset": 1,
            "regexp": "BIGACE ([\\d.]+)"
        }
    ],
    "website": "http://bigace.de",
    "implies": "PHP"
}