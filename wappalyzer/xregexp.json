{
    "name": "XRegExp",
    "website": "http://xregexp.com",
    "matches": [
        {
            "regexp": "xregexp[.-]([\\d.]*\\d)[^/]*\\.js",
            "search": "script",
            "offset": 1
        },
        {
            "regexp": "/([\\d.]+)/xregexp(?:\\.min)?\\.js",
            "search": "script",
            "offset": 1
        },
        {
            "regexp": "xregexp.*\\.js",
            "search": "script"
        }
    ]
}