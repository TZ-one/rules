{
    "name": "Google Analytics",
    "website": "http://google.com/analytics",
    "matches": [
        {
            "offset": 1,
            "search": "script",
            "regexp": "gtag\\([^)]+'(UA-)"
        },
        {
            "offset": 1,
            "search": "script",
            "regexp": "gtag\\([^)]+'(G-)"
        },
        {
            "search": "cookies[_gat]",
            "regexp": ".*"
        },
        {
            "search": "cookies[__utma]",
            "regexp": ".*"
        },
        {
            "search": "cookies[_ga]",
            "regexp": ".*"
        }
    ]
}