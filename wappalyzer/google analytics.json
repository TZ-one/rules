{
    "name": "Google Analytics",
    "website": "http://google.com/analytics",
    "matches": [
        {
            "search": "cookies[_gat]",
            "regexp": ".*"
        },
        {
            "search": "cookies[__utma]",
            "regexp": ".*"
        },
        {
            "search": "cookies[_ga]",
            "regexp": ".*"
        },
        {
            "search": "cookies[_ga_*]",
            "regexp": ".*"
        },
        {
            "search": "script",
            "regexp": "gtag\\([^)]+'(UA-)",
            "offset": 1
        },
        {
            "search": "script",
            "regexp": "gtag\\([^)]+'(G-)",
            "offset": 1
        }
    ]
}