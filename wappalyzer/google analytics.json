{
    "name": "Google Analytics",
    "website": "http://google.com/analytics",
    "matches": [
        {
            "regexp": "gtag\\([^)]+'(UA-)",
            "offset": 1,
            "search": "script"
        },
        {
            "regexp": "gtag\\([^)]+'(G-)",
            "offset": 1,
            "search": "script"
        },
        {
            "regexp": ".*",
            "search": "cookies[_gat]"
        },
        {
            "regexp": ".*",
            "search": "cookies[_ga]"
        },
        {
            "regexp": ".*",
            "search": "cookies[_ga_*]"
        },
        {
            "regexp": ".*",
            "search": "cookies[__utma]"
        }
    ]
}