{
    "name": "Microsoft Publisher",
    "website": "https://office.microsoft.com/publisher",
    "matches": [
        {
            "regexp": "(?:<html [^>]*xmlns:w=\"urn:schemas-microsoft-com:office:publisher\"|<!--[if pub]><xml>)"
        },
        {
            "regexp": "^Publisher\\.",
            "search": "meta[ProgId]"
        },
        {
            "offset": 1,
            "regexp": "Microsoft Publisher( [\\d.]+)?",
            "search": "meta[generator]"
        }
    ]
}