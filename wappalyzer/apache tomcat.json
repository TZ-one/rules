{
    "name": "Apache Tomcat",
    "implies": "Java",
    "website": "http://tomcat.apache.org",
    "matches": [
        {
            "search": "headers[server]",
            "regexp": "^Apache-Coyote"
        },
        {
            "offset": 1,
            "search": "headers[x-powered-by]",
            "regexp": "\\bTomcat\\b(?:-([\\d.]+))?"
        }
    ]
}