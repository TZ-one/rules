{
    "name": "Virtuoso",
    "matches": [
        {
            "search": "meta[Keywords]",
            "regexp": "^OpenLink Virtuoso Sparql"
        },
        {
            "search": "meta[Copyright]",
            "regexp": "^Copyright &copy; \\d{4} OpenLink Software"
        },
        {
            "search": "headers[server]",
            "regexp": "Virtuoso/?([0-9.]+)?",
            "offset": 1
        }
    ],
    "website": "https://virtuoso.openlinksw.com/"
}