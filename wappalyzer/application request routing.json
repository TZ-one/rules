{
    "name": "Application Request Routing",
    "implies": "IIS",
    "matches": [
        {
            "search": "headers[x-powered-by]",
            "regexp": "^ARR/([\\d\\.]+)$",
            "offset": 1
        }
    ],
    "website": "https://www.iis.net/downloads/microsoft/application-request-routing"
}