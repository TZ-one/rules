{
    "name": "Foswiki",
    "implies": "Perl",
    "matches": [
        {
            "search": "meta[foswiki.SERVERTIME]",
            "regexp": ".*"
        },
        {
            "search": "meta[foswiki.WIKINAME]",
            "regexp": ".*"
        },
        {
            "search": "headers[x-foswikiuri]",
            "regexp": ".*"
        },
        {
            "search": "headers[x-foswikiaction]",
            "regexp": ".*"
        },
        {
            "search": "cookies[SFOSWIKISID]",
            "regexp": ".*"
        },
        {
            "search": "cookies[FOSWIKISTRIKEONE]",
            "regexp": ".*"
        },
        {
            "regexp": "<div class=\"foswiki(?:Copyright|Page|Main)\">"
        }
    ],
    "website": "http://foswiki.org"
}