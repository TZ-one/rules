{
    "name": "React",
    "website": "https://reactjs.org",
    "matches": [
        {
            "regexp": "<[^>]+data-react"
        },
        {
            "search": "script",
            "offset": 1,
            "regexp": "react(?:-with-addons)?[.-]([\\d.]*\\d)[^/]*\\.js"
        },
        {
            "search": "script",
            "offset": 1,
            "regexp": "/([\\d.]+)/react(?:\\.min)?\\.js"
        },
        {
            "search": "script",
            "regexp": "^react\\b.*\\.js"
        }
    ]
}