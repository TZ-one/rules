{
    "name": "Amazon S3",
    "website": "http://aws.amazon.com/s3/",
    "implies": "Amazon Web Services",
    "matches": [
        {
            "regexp": "^AmazonS3$",
            "search": "headers[server]"
        },
        {
            "regexp": "s3[^ ]*\\.amazonaws\\.com\\s",
            "search": "headers[content-security-policy-report-only]"
        },
        {
            "regexp": "s3[^ ]*amazonaws\\.com\\s",
            "search": "headers[content-security-policy]"
        }
    ]
}