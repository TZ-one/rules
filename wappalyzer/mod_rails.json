{
    "name": "mod_rails",
    "website": "http://phusionpassenger.com",
    "implies": [
        "Ruby on Rails",
        "Apache"
    ],
    "matches": [
        {
            "search": "headers[x-powered-by]",
            "regexp": "mod_rails(?:/([\\d.]+))?",
            "offset": 1
        },
        {
            "search": "headers[server]",
            "regexp": "mod_rails(?:/([\\d.]+))?",
            "offset": 1
        }
    ]
}