{
    "name": "Web Stories for WordPress",
    "matches": [
        {
            "search": "meta[amp-story-generator-version]",
            "offset": 1,
            "regexp": "^(.+)$"
        },
        {
            "search": "meta[amp-story-generator-name]",
            "regexp": "^Web Stories for WordPress$"
        }
    ],
    "implies": "Web Stories",
    "website": "https://wp.stories.google"
}