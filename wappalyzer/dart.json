{
    "website": "https://www.dartlang.org",
    "name": "Dart",
    "excludes": [
        "Angular",
        "AngularJS"
    ],
    "matches": [
        {
            "search": "script",
            "regexp": "/(?:\\.)?(?:dart)(?:\\.js)?/"
        },
        {
            "search": "script",
            "regexp": "packages/browser/dart\\.js"
        },
        {
            "regexp": "/(?:<script)[^>]+(?:type=\"application/dart\")/"
        }
    ],
    "implies": "AngularDart"
}