{
    "name": "Dart",
    "website": "https://www.dartlang.org",
    "excludes": [
        "Angular",
        "AngularJS"
    ],
    "matches": [
        {
            "regexp": "/(?:<script)[^>]+(?:type=\"application/dart\")/"
        },
        {
            "regexp": "/(?:\\.)?(?:dart)(?:\\.js)?/",
            "search": "script"
        },
        {
            "regexp": "packages/browser/dart\\.js",
            "search": "script"
        }
    ],
    "implies": "AngularDart"
}