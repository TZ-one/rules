{
    "name": "Nginx",
    "matches": [
        {
            "search": "headers[x-fastcgi-cache]",
            "regexp": ".*"
        },
        {
            "search": "headers[server]",
            "regexp": "nginx(?:/([\\d.]+))?",
            "offset": 1
        }
    ],
    "website": "http://nginx.org/en"
}