{
    "website": "http://umbraco.com",
    "implies": "Microsoft ASP.NET",
    "matches": [
        {
            "regexp": "umbraco",
            "search": "meta[generator]"
        },
        {
            "regexp": "powered by <a href=[^>]+umbraco"
        },
        {
            "regexp": "^(.+)$",
            "search": "headers[x-umbraco-version]",
            "offset": 1
        }
    ],
    "name": "Umbraco"
}