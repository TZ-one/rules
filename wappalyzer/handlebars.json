{
    "name": "Handlebars",
    "website": "http://handlebarsjs.com",
    "matches": [
        {
            "regexp": "<[^>]*type=[^>]text\\/x-handlebars-template"
        },
        {
            "search": "script",
            "offset": 1,
            "regexp": "handlebars(?:\\.runtime)?(?:-v([\\d.]+?))?(?:\\.min)?\\.js"
        }
    ]
}