{
    "name": "vBulletin",
    "website": "https://www.vbulletin.com",
    "implies": "PHP",
    "matches": [
        {
            "regexp": "<div id=\"copyright\">Powered by vBulletin"
        },
        {
            "search": "cookies[bblastactivity]",
            "regexp": ".*"
        },
        {
            "search": "cookies[bblastvisit]",
            "regexp": ".*"
        },
        {
            "search": "cookies[bbsessionhash]",
            "regexp": ".*"
        },
        {
            "offset": 1,
            "search": "meta[generator]",
            "regexp": "vBulletin ?([\\d.]+)?"
        }
    ]
}