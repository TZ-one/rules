{
    "name": "Ghost",
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-ghost-cache-status]"
        },
        {
            "regexp": "Ghost(?:\\s([\\d.]+))?",
            "search": "meta[generator]",
            "offset": 1
        }
    ],
    "implies": "Node.js",
    "website": "http://ghost.org"
}