{
    "implies": "Microsoft ASP.NET",
    "name": "Smartstore",
    "website": "https://www.smartstore.com",
    "matches": [
        {
            "regexp": "<!--Powered by Smart[sS]tore"
        },
        {
            "regexp": "<meta property=\"sm:pagedata\""
        },
        {
            "regexp": ".*",
            "search": "cookies[SMARTSTORE.VISITOR]"
        },
        {
            "regexp": ".*",
            "search": "cookies[SMARTSTORE.CUSTOMER]"
        },
        {
            "regexp": "^Smart[sS]tore(.NET)? (.+)$",
            "search": "meta[generator]",
            "offset": 2
        }
    ]
}