{
    "name": "Outlook Web App",
    "matches": [
        {
            "search": "headers[x-owa-version]",
            "offset": 1,
            "regexp": "([\\d\\.]+)?"
        },
        {
            "regexp": "<link[^>]+/owa/auth/([\\d\\.]+)/themes/resources",
            "offset": 1
        }
    ],
    "website": "http://help.outlook.com",
    "implies": "Microsoft ASP.NET"
}