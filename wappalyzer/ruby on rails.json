{
    "name": "Ruby on Rails",
    "implies": "Ruby",
    "website": "https://rubyonrails.org",
    "matches": [
        {
            "search": "headers[server]",
            "regexp": "mod_(?:rails|rack)"
        },
        {
            "search": "headers[x-powered-by]",
            "regexp": "mod_(?:rails|rack)"
        },
        {
            "search": "cookies[_session_id]",
            "regexp": ".*",
            "certainty": 75
        },
        {
            "search": "meta[csrf-param]",
            "regexp": "^authenticity_token$",
            "certainty": 50
        }
    ]
}