{
    "name": "Debian",
    "matches": [
        {
            "search": "headers[server]",
            "regexp": "Debian"
        },
        {
            "offset": 1,
            "search": "headers[x-powered-by]",
            "regexp": "(?:Debian|dotdeb|(potato|woody|sarge|etch|lenny|squeeze|wheezy|jessie|stretch|buster|sid))"
        }
    ],
    "website": "https://debian.org"
}