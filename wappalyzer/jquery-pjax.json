{
    "implies": "jQuery",
    "name": "jQuery-pjax",
    "matches": [
        {
            "regexp": "<div[^>]+data-pjax-container"
        },
        {
            "regexp": ".*",
            "search": "meta[pjax-push]"
        },
        {
            "regexp": ".*",
            "search": "meta[pjax-replace]"
        },
        {
            "regexp": ".*",
            "search": "meta[pjax-timeout]"
        }
    ],
    "website": "https://github.com/defunkt/jquery-pjax"
}