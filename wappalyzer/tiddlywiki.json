{
    "name": "TiddlyWiki",
    "website": "http://tiddlywiki.com",
    "matches": [
        {
            "regexp": "^TiddlyWiki$",
            "search": "meta[application-name]"
        },
        {
            "regexp": "^TiddlyWiki$",
            "search": "meta[generator]"
        },
        {
            "regexp": "^TiddlyWiki created by Jeremy Ruston",
            "search": "meta[copyright]"
        },
        {
            "regexp": "^(.+)$",
            "offset": 1,
            "search": "meta[tiddlywiki-version]"
        },
        {
            "regexp": "<[^>]*type=[^>]text\\/vnd\\.tiddlywiki"
        }
    ]
}