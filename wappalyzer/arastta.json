{
    "excludes": "OpenCart",
    "implies": "PHP",
    "website": "http://arastta.org",
    "name": "Arastta",
    "matches": [
        {
            "regexp": "Powered by <a [^>]*href=\"https?://(?:www\\.)?arastta\\.org[^>]+>Arastta"
        },
        {
            "regexp": "^(.+)$",
            "search": "headers[arastta]",
            "offset": 1
        },
        {
            "regexp": ".*",
            "search": "headers[x-arastta]"
        }
    ]
}