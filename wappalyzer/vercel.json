{
    "name": "Vercel",
    "website": "https://vercel.com",
    "matches": [
        {
            "search": "headers[x-vercel-id]",
            "regexp": ".*"
        },
        {
            "search": "headers[x-now-trace]",
            "regexp": ".*"
        },
        {
            "search": "headers[x-vercel-cache]",
            "regexp": ".*"
        },
        {
            "search": "headers[server]",
            "regexp": "^now|Vercel$"
        }
    ]
}