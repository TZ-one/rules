{
    "website": "https://www.commercejs.com",
    "name": "Commerce.js",
    "matches": [
        {
            "regexp": "cdn\\.chec\\.io/v(\\d+)/commerce\\.js",
            "search": "script",
            "offset": 1
        },
        {
            "regexp": "chec/commerce\\.js",
