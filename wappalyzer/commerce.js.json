{
    "website": "https://www.commercejs.com",
    "name": "Commerce.js",
    "matches": [
        {
            "search": "headers[x-powered-by]",
            "regexp": "Commerce.js"
        },
        {
            "search": "headers[chec-version]",
            "regexp": ".*"
        },
        {
            "search": "script",
            "regexp": "cdn\\.chec\\.io/v(\\d+)/commerce\\.js",
            "offset": 1
        },
        {
            "search": "script",
            "regexp": "chec/commerce\\.js"
        }
    ]
}