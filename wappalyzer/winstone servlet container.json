{
    "name": "Winstone Servlet Container",
    "matches": [
        {
            "search": "headers[x-powered-by]",
            "regexp": "Winstone(?:\\/([\\d.]+))?",
            "offset": 1
        },
        {
            "search": "headers[server]",
            "regexp": "Winstone Servlet (?:Container|Engine) v?([\\d.]+)?",
            "offset": 1
        }
    ],
    "website": "http://winstone.sourceforge.net"
}