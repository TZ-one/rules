{
    "name": "Microsoft ASP.NET",
    "implies": "IIS",
    "website": "https://www.asp.net",
    "matches": [
        {
            "regexp": "<input[^>]+name=\"__VIEWSTATE"
        },
        {
            "regexp": ".*",
            "search": "cookies[ASPSESSION]"
        },
        {
            "regexp": ".*",
            "search": "cookies[ASP.NET_SessionId]"
        },
        {
            "regexp": "(.+)",
            "offset": 1,
            "search": "headers[x-aspnet-version]"
        },
        {
            "regexp": "^ASP\\.NET",
            "search": "headers[x-powered-by]"
        }
    ]
}