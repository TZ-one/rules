{
    "name": "ImpressCMS",
    "website": "http://www.impresscms.org",
    "implies": "PHP",
    "matches": [
        {
            "regexp": ".*",
            "search": "cookies[ImpressCMS]"
        },
        {
            "regexp": ".*",
            "search": "cookies[ICMSSession]"
        },
        {
            "regexp": "ImpressCMS",
            "search": "meta[generator]"
        },
        {
            "regexp": "include/linkexternal\\.js",
            "search": "script"
        }
    ]
}