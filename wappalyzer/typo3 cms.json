{
    "implies": "PHP",
    "matches": [
        {
            "regexp": "<link[^>]+ href=\"/?typo3(?:conf|temp)/"
        },
        {
            "regexp": "<img[^>]+ src=\"/?typo3(?:conf|temp)/"
        },
        {
            "regexp": "<!--\n\tThis website is powered by TYPO3"
        },
        {
            "regexp": "^/?typo3(?:conf|temp)/",
            "search": "script"
        },
        {
            "regexp": "TYPO3\\s+(?:CMS\\s+)?(?:[\\d.]+)?(?:\\s+CMS)?",
            "search": "meta[generator]"
        }
    ],
    "name": "TYPO3 CMS",
    "website": "https://typo3.org/"
}