{
    "implies": "PHP",
    "website": "https://www.dokuwiki.org",
    "name": "DokuWiki",
    "matches": [
        {
            "regexp": "<div[^>]+id=\"dokuwiki__>"
        },
        {
            "regexp": "<a[^>]+href=\"#dokuwiki__"
        },
        {
            "search": "cookies[DokuWiki]",
            "regexp": ".*"
        },
        {
            "search": "meta[generator]",
            "regexp": "^DokuWiki( Release [\\d-]+)?",
            "offset": 1
        }
    ]
}