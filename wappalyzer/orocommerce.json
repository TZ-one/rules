{
    "name": "OroCommerce",
    "website": "https://oroinc.com",
    "implies": [
        "PHP",
        "MySQL"
    ],
    "matches": [
        {
            "regexp": "<script [^>]+data-requiremodule=\"oro/"
        },
        {
            "regexp": "<script [^>]+data-requiremodule=\"oroui/"
        },
        {
            "offset": 1,
            "regexp": "oro\\.min\\.js\\?version=([\\d.]+)",
            "search": "script"
        }
    ]
}