{
    "name": "SonarQubes",
    "implies": "Java",
    "website": "https://www.sonarqube.org/",
    "matches": [
        {
            "search": "script",
            "offset": 1,
            "regexp": "^/js/bundles/sonar\\.js?v=([\\d.]+)$"
        },
        {
            "offset": 1,
            "regexp": "<link href=\"/css/sonar\\.css\\?v=([\\d.]+)"
        },
        {
            "regexp": "<title>SonarQube</title>"
        },
        {
            "search": "meta[application-name]",
            "regexp": "^SonarQubes$"
        }
    ]
}