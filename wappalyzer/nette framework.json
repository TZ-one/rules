{
    "name": "Nette Framework",
    "website": "https://nette.org",
    "implies": "PHP",
    "matches": [
        {
            "regexp": ".*",
            "search": "cookies[nette-browser]"
        },
        {
            "regexp": "<input[^>]+data-nette-rules"
        },
        {
            "regexp": "<div[^>]+id=\"snippet-"
        },
        {
            "regexp": "<input[^>]+id=\"frm-"
        },
        {
            "regexp": "^Nette Framework",
            "search": "headers[x-powered-by]"
        }
    ]
}