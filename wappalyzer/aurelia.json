{
    "name": "Aurelia",
    "website": "http://aurelia.io",
    "matches": [
        {
            "search": "script",
            "regexp": "aurelia(?:\\.min)?\\.js"
        },
        {
            "regexp": "<[^>]+aurelia-app=[^>]"
        },
        {
            "regexp": "<[^>]+data-main=[^>]aurelia-bootstrapper"
        },
        {
            "regexp": "<[^>]+au-target-id=[^>]\\d"
        }
    ]
}