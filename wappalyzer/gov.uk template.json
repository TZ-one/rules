{
    "website": "https://github.com/alphagov/govuk_template/",
    "name": "GOV.UK Template",
    "matches": [
        {
            "search": "script",
            "regexp": "govuk-template\\.js"
        },
        {
            "regexp": "<link[^>]+govuk-template[^>\"]+css"
        },
        {
            "regexp": "<link[^>]+govuk-template-print[^>\"]+css"
        },
        {
            "regexp": "<link[^>]+govuk-template-ie6[^>\"]+css"
        },
        {
            "regexp": "<link[^>]+govuk-template-ie7[^>\"]+css"
        },
        {
            "regexp": "<link[^>]+govuk-template-ie8[^>\"]+css"
        }
    ]
}