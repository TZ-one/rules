{
    "name": "Pantheon",
    "website": "https://pantheon.io/",
    "implies": [
        "PHP",
        "Nginx",
        "MariaDB",
        "Fastly"
    ],
    "matches": [
        {
            "regexp": ".*",
            "search": "headers[x-styx-req-id]"
        },
        {
            "regexp": ".*",
            "search": "headers[x-pantheon-styx-hostname]"
        },
        {
            "regexp": "^Pantheon",
            "search": "headers[server]"
        }
    ]
}