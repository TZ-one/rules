{
    "name": "Shapecss",
    "website": "https://shapecss.com",
    "matches": [
        {
            "regexp": "<link[^>]* href=\"[^\"]*shapecss(?:\\.min)?\\.css"
        },
        {
            "regexp": "shapecss[-.]([\\d.]*\\d)[^/]*\\.js",
            "search": "script",
            "offset": 1
        },
        {
            "regexp": "/([\\d.]+)/shapecss(?:\\.min)?\\.js",
            "search": "script",
            "offset": 1
        },
        {
            "regexp": "shapecss.*\\.js",
            "search": "script"
        }
    ]
}