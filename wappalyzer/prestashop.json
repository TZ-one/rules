{
    "name": "PrestaShop",
    "matches": [
        {
            "regexp": "PrestaShop",
            "search": "meta[generator]"
        },
        {
            "regexp": ".*",
            "search": "cookies[PrestaShop]"
        },
        {
            "regexp": "^Prestashop$",
            "search": "headers[powered-by]"
        },
        {
            "regexp": "Powered by <a\\s+[^>]+>PrestaShop"
        },
        {
            "regexp": "<!-- /Block [a-z ]+ module (?:HEADER|TOP)?\\s?-->"
        },
        {
            "regexp": "<!-- /Module Block [a-z ]+ -->"
        }
    ],
    "website": "http://www.prestashop.com",
    "implies": [
        "PHP",
        "MySQL"
    ]
}