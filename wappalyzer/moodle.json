{
    "name": "Moodle",
    "implies": "PHP",
    "website": "http://moodle.org",
    "matches": [
        {
            "regexp": "<img[^>]+moodlelogo"
        },
        {
            "search": "cookies[MOODLEID_]",
            "regexp": ".*"
        },
        {
            "search": "cookies[MoodleSession]",
            "regexp": ".*"
        },
        {
            "search": "meta[keywords]",
            "regexp": "^moodle"
        }
    ]
}