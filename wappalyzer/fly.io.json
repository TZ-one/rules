{
    "name": "Fly.io",
    "website": "https://fly.io",
    "matches": [
        {
            "regexp": "^Fly/[\\w]+\\s\\(.*\\)$",
            "search": "headers[server]"
        },
        {
            "regexp": "^.*\\sfly\\.io$",
            "search": "headers[via]"
        },
        {
            "regexp": ".*",
            "search": "headers[fly-request-id]"
        },
        {
            "regexp": ".*",
            "search": "cookies[_fly]"
        }
    ]
}