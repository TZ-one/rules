{
    "name": "Centra",
    "matches": [
        {
            "search": "script",
            "regexp": "centraCheckoutScript"
        },
        {
            "search": "headers[content-security-policy]",
            "regexp": "\\.centra(?:cdn)?\\.(?:com|net)"
        }
    ],
    "website": "https://centra.com",
    "excludes": "Magento"
}