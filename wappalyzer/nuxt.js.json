{
    "website": "https://nuxtjs.org",
    "implies": [
        "Vue.js",
        "Node.js"
    ],
    "name": "Nuxt.js",
    "matches": [
        {
            "regexp": "/_nuxt/",
            "search": "script"
        },
        {
            "regexp": "<div [^>]*id=\"__nuxt\""
        },
        {
            "regexp": "<script [^>]*>window\\.__NUXT__"
        }
    ]
}